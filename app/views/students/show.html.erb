<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @student.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @student.email %>
</p>

<p>
  <strong>Major:</strong>
  <%= @student.major %>
</p>

<p>
	<strong>Courses Enrolled:</strong>
	<ul>
    <% @student.courses.each do |course| %>
    <li><%= course.name %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', students_path %>
